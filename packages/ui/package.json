{
  "name": "@axonicles/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/styles.css",
    "./*": "./src/*.tsx"
  },
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "lint": "eslint src/",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@axonicles/eslint-config": "*",
    "@axonicles/utils": "*",
    "@axonicles/typescript-config": "*",
    "@turbo/gen": "^2.5.0",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.26.0",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@axonicles/tailwind-config": "*",
    "@radix-ui/react-tooltip": "^1.2.7",
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
